//Find the point
#include <bits/stdc++.h>

using namespace std;

string ltrim(const string &);
string rtrim(const string &);
vector<string> split(const string &);

/*
 parameters:
 *  1. INTEGER px
 *  2. INTEGER py
 *  3. INTEGER qx
 *  4. INTEGER qy
 */

int main()
{
    int n;
    cin>>n;
    for(int i=0;i<n;++i)
    {
        int px,py,qx,qy;
        cin>>px>>py>>qx>>qy;
        cout<<(2*qx-px)<<" "<<(2*qy-py)<<"\n";//midpoint formula arrangement
    }
    
        
    }
